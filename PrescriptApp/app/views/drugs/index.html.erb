<h1>Drugs</h1>

<div><h2>Results</h2></div>

<table>
	<thead>
		<tr>
		<% @result.columns.each do |title| %>
			<th><%= title %></th>
		<% end %>
		</tr>
	</thead>

	<tbody>
		<% @result.rows.each do |row| %>
		<tr>
		<% row.each do |value| %>
			<td><%= value %></td>
		<% end %>
		</tr>
		<% end %>
	</tbody>
</table>

<%= render 'doctor' %>
<%= render 'patient' %>
<%= render 'pharmacist' %>

<%= link_to "Home", root_path %>

			